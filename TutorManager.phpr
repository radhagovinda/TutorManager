			<query>
				<m_sql>SELECT
students.StudentID,
students.Photo,
students.SalutationID,
students.FirstName,
students.MiddleName,
students.LastName,
trim(concat(`salutations`.`Salutation`,' ',
`students`.`FirstName`,' ',
`students`.`MiddleName`,' ',
`students`.`LastName`)) AS FullName,
students.FathersName,
students.Gender,
students.DOB,
students.CurrentlyStudyingAt,
students.MobileNumber,
students.FathersMobileNumber,
students.HomePhoneNumber,
students.PresentAddress,
students.City,
students.PINCode,
students.AdmittedOn,
students.IsActive,
0 AS BatchID,
`class`.`Class`,
batch.BatchName,
subjects.Subject
FROM students
LEFT OUTER JOIN salutations ON students.SalutationID = salutations.ID
INNER JOIN student_batch ON students.StudentID = student_batch.StudentID
INNER JOIN batch ON student_batch.BatchID = batch.BatchID
INNER JOIN `class` ON batch.ClassID = `class`.ClassID
INNER JOIN subjects ON batch.SubjectID = subjects.SubjectID
</m_sql>
