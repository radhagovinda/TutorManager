			<m_strSQL>SELECT
student_batch.ID,
student_batch.AcademicYear,
student_batch.BatchID,
student_batch.StudentID,
concat(`salutations`.`Salutation`, ' ', `students`.`FirstName`, ' ', `students`.`MiddleName`, ' ', `students`.`LastName`) AS FullName,
concat(`batch`.`BatchName`, ' (', `class`.`Class`, '-', `subjects`.`Subject`, ')') AS FullBatchName,
subjects.Subject,
`class`.`Class`,
batch.BatchName,
0 AS MarksObtained
FROM student_batch
LEFT OUTER JOIN students ON student_batch.StudentID = students.StudentID
LEFT OUTER JOIN salutations ON students.SalutationID = salutations.ID
INNER JOIN batch ON student_batch.BatchID = batch.BatchID
INNER JOIN `class` ON batch.ClassID = `class`.ClassID
INNER JOIN subjects ON batch.SubjectID = subjects.SubjectID
</m_strSQL>
