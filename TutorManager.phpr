					<m_strEventCode>global $dal;
global $conn;


if ($_REQUEST["a"]=="added") {
$rstmp = CustomQuery($_SESSION["sqlGetDetailsFull"]);
while ($datatmp = db_fetch_array($rstmp)) {
	//=======================================================================
		//========== Fetching Maximum Marks START=================
						if ($MaxMarks =='') {
						$strSQLExists = "select max(MarksObtained) as MaxMark from examresult where ExamID='".$datatmp['ExamID']."'";
						$rsExists = db_query($strSQLExists,$conn);
						$data=db_fetch_array($rsExists);						
						$MaxMarks = $data['MaxMark'];
						}

						//$tblExamResult = $dal-&gt;Table("examresult");
						//$thequery = "select max(MarksObtained) as MaxMark from examresult where ExamID='".$datatmp['ExamID']."'";
						//$rs = $tblExamResult-&gt;Query($thequery);
						//$data = db_fetch_array($rs);
						//$MaxMarks = $data['MaxMark'];  
		//========== Fetching Maximum Marks END=================


	$FullName = $datatmp['FullName'];
	$ExamName = $datatmp['ExamName'];
	$MarksObtained = $datatmp['MarksObtained'];
	$MobileNumbers = '91'.$datatmp['MobileNumber'].',91'.$datatmp['FathersMobileNumber'];
	$FullMarks = $datatmp['FullMarks'];
	$ExamDate = date("d-m-Y", strtotime($datatmp['ExamDate']));
	//$ExamDate = $datatmp['ExamDate'];
	$Subject = $datatmp['Subject'];

	$MessageRaw = "Dear &lt;StudentNameVariable&gt;, you obtained &lt;marksObtainedVariable&gt; out of &lt;totalmarksVariable&gt; in the &lt;SubjectVariable&gt;  mock test held on &lt;dateVariable&gt;. Highest Mark: &lt;highestMarksVariable&gt;. - Pradip Sir";

	$MessageRawA = str_replace("&lt;StudentNameVariable&gt;", $FullName, $MessageRaw);
	$MessageRawB = str_replace("&lt;marksObtainedVariable&gt;", $MarksObtained, $MessageRawA);
	$MessageRawC = str_replace("&lt;totalmarksVariable&gt;", $FullMarks, $MessageRawB);
	$MessageRawD = str_replace("&lt;totalmarksVariable&gt;", $FullMarks, $MessageRawC);
	$MessageRawE = str_replace("&lt;SubjectVariable&gt;", $Subject, $MessageRawD);
	$MessageRawF = str_replace("&lt;dateVariable&gt;", $ExamDate, $MessageRawE);
	$MessageRawG = str_replace("&lt;highestMarksVariable&gt;", $MaxMarks, $MessageRawF);

	//====================== START SMS Sending==================
							$senderId = $_SESSION['SMSSenderId'];
							$username = $_SESSION['SMSUserId'];
							$password = $_SESSION['SMSPassword'];
							//Your message to send, Add URL endcoding here.
							// init the resource
							$url = 'http://203.212.70.200/smpp/sendsms';
							$myvars = 'username=' . $username . '&amp;password=' . $password. '&amp;to=' . $MobileNumbers . '&amp;from=' . $senderId . '&amp;udh=' . '&amp;text=' . $MessageRawG . '&amp;dlr-mask=19&amp;dlr-url&amp;category=bulk' ;
						
							$ch = curl_init( $url );
							curl_setopt( $ch, CURLOPT_POST, 1);
							curl_setopt( $ch, CURLOPT_POSTFIELDS, $myvars);
							curl_setopt( $ch, CURLOPT_FOLLOWLOCATION, 1);
							curl_setopt( $ch, CURLOPT_HEADER, 0);
							curl_setopt( $ch, CURLOPT_RETURNTRANSFER, 1);

							$response = curl_exec( $ch );

							curl_close($ch);
							echo $response;
							// Nullyfying table variable values
							$FullName = '';
							$ExamName = '';
							$MarksObtained = '';
							$MobileNumbers = '';
							$FullMarks = '';
							$ExamDate = '';
							$Subject = '';
							
	//====================== END SMS Sending==================

}
	// Nullyfying message variable values
	$MaxMarks ='';
	$MessageRaw ='';
	$MessageRawA ='';
	$MessageRawB = '';
	$MessageRawC = '';
	$MessageRawD = '';
	$MessageRawE = '';
	$MessageRawF = '';
	$MessageRawG = '';

echo "Marks sent to all the students and their guardians&lt;br&gt;";
echo "&lt;p&gt;&lt;a href='javascript:window.close();'&gt;Close window&lt;/a&gt;&lt;/p&gt;";
	//=======================================================================
exit();

} else {
$sqlGetDetails = $_SESSION["SQLforGetDetails"] ;

  
  for ($i=0;$i&lt;count($_REQUEST["selection"]);$i++ )
    $sqlGetDetails.=$_REQUEST["selection"][$i].",";
    
   $sqlGetDetails = substr($sqlGetDetails,0,-1);
    $sqlGetDetails.= ")";
$_SESSION["sqlGetDetailsFull"] = $sqlGetDetails;
}
