			<query>
				<m_sql>SELECT
student_batch.ID,
students.Photo,
student_batch.StudentID,
concat(`salutations`.`Salutation`, ' ', `students`.`FirstName`, '', `students`.`MiddleName`, ' ', `students`.`LastName`) AS FullName,
student_batch.AcademicYear,
student_batch.BatchID,
batch.BatchName,
concat(batch.BatchName, ' (', class.Class, ', ', subjects.Subject, ') ') AS BatchFullName,
students.MobileNumber,
students.FathersMobileNumber,
`class`.ClassID,
`class`.`Class`,
subjects.SubjectID,
subjects.Subject,
fees_table.Fees
FROM student_batch
LEFT OUTER JOIN students ON student_batch.StudentID = students.StudentID
LEFT OUTER JOIN batch ON student_batch.BatchID = batch.BatchID
LEFT OUTER JOIN `class` ON batch.ClassID = `class`.ClassID
LEFT OUTER JOIN subjects ON batch.SubjectID = subjects.SubjectID
LEFT OUTER JOIN fees_table ON subjects.SubjectID = fees_table.SubjectID AND `class`.ClassID = fees_table.ClassID
LEFT OUTER JOIN salutations ON students.SalutationID = salutations.ID
</m_sql>
